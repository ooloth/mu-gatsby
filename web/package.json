    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "dev": "rm -rf .cache && gatsby develop",
    "format": "npm run prettier -- --write",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|html|md|mdx|graphql)\" --ignore-path .gitignore",
    "serve": "npm run build:local && gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm-run-all --parallel check-types check-format lint"
